    "require-yaml": "^0.0.1",
    "run-sequence": "^2.2.1",
    "stylelint": "^9.10.1",
    "stylelint-config-prettier": "^5.0.0",
    "stylelint-order": "^2.1.0",
    "stylelint-selector-bem-pattern": "^2.0.0",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "webpack": "^4.20.2",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "inputmask": "^3.3.11",
    "jquery": "^3.1.1",
    "jquery-migrate": "^3.0.1",
    "jquery.equalheights": "^1.5.3",
    "nouislider": "^10.1.0",
    "select2": "4.0.5",
    "simplebar": "^3.1.5",
    "smart-grid": "^2.1.2",
    "sticky-kit": "^1.1.3",
    "sticky-sidebar": "^3.3.1",
    "svg4everybody": "^2.1.9",
    "swiper": "^4.5.0"
  }
}
