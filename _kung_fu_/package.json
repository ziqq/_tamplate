{
  "name": "kanscraft",
  "version": "0.0.1",
  "author": "Anton Ustinoff <a.a.ustinoff@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/spitiru/kanscraft-markup"
  },
  "scripts": {
    "ghpages": "git subtree push --prefix build origin gh-pages",
    "start": "gulp",
    "build": "gulp build:production --production",
    "precommit": "lint-staged",
    "commit": "git-cz",
    "eslint": "eslint --format=node_modules/eslint-formatter-pretty src/js/",
    "eslint-fix": "npm run eslint -- --fix",
    "style:lint": "stylelint src/sass/**/*.scss --config ./.stylelintrc",
    "style:fix": "stylelint src/sass/**/*.scss --fix --config ./.stylelintrc",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "linters": {
      "src/templates/**/*.html": [
        "htmlhint --config .htmlhintrc"
      ],
      "src/js/**/*.{js, jsx}": [
        "node_modules/.bin/eslint",
        "prettier --write",
        "git add"
      ],
      "src/sass/**/*.scss": [
        "stylelint --fix --config ./.stylelintrc",
        "prettier --write",
        "git add"
      ]
    }
  },
  "devDependencies": {
    "@babel/polyfill": "^7.4.0",
    "autoprefixer": "^7.1.6",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^8.0.4",
    "babel-plugin-transform-runtime": "^6.4.3",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.3.13",
    "browser-sync": "^2.10.0",
    "clean-css": "^4.2.1",
    "commitizen": "^3.0.7",
    "conventional-changelog-cli": "^2.0.12",
    "css-mqpacker": "^6.0.1",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^4.1.0",
    "eslint-formatter-pretty": "^2.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^7.0.0",
    "gulp-cached": "^1.1.1",
    "gulp-changed": "^3.1.0",
    "gulp-cheerio": "^0.6.2",
    "gulp-concat": "^2.6.1",
    "gulp-consolidate": "^0.2.0",
    "gulp-front-matter": "^1.3.0",
    "gulp-htmlhint": "^0.3.1",
    "gulp-if": "^2.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-include": "^2.0.3",
    "gulp-inject-string": "^1.1.0",
    "gulp-newer": "^1.4.0",
    "gulp-notify": "^3.0.0",
    "gulp-nunjucks-render": "^2.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-postcss": "^7.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-stylelint": "^8.0.0",
    "gulp-svg-sprite": "^1.3.7",
    "gulp-svgmin": "^1.2.0",
    "gulp-svgstore": "^6.1.0",
    "gulp-uglify": "^3.0.1",
    "gulp-util": "^3.0.7",
    "gulp-watch": "^5.0.1",
    "gulp.spritesmith": "^6.0.0",
    "htmlhint": "^0.9.13",
    "husky": "^0.14.3",
    "imagemin-pngquant": "^5.1.0",
    "lint-staged": "^4.0.2",
    "lodash": "^4.3.0",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "postcss-csso": "^3.0.0",
    "prettier": "^1.16.4",
